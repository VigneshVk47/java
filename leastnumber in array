import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int k=0;
	Scanner scan=new Scanner(System.in);
	int n;
	n=scan.nextInt();
	int arr[]=new int[n];
	for(int i=0;i<n;i++){
	    arr[i]=scan.nextInt();
	   
	}k=arr[0];
	    for(int j=0;j<n;j++){
	        if(arr[j]<k){
	            k=arr[j];
	        }
	    }System.out.println(k);
	}}
